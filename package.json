  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "eslint": "^7.26.0",
    "lerna": "^4.0.0"
  },
  "scripts": {
